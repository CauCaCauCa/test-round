{
  "name": "nest-postgres",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "npx drizzle-kit generate --config=src/_core/config/database/postgres/drizzle-kit.ts",
    "db:migrate": "npx drizzle-kit migrate --config=src/_core/config/database/postgres/drizzle-kit.ts",
    "db:pull": "npx drizzle-kit pull --config=src/_core/config/database/postgres/drizzle-kit.ts",
    "deploy": "yarn run build && serverless deploy",
    "undeploy": "serverless remove"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/fastify": "^3.1.0",
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/client-sqs": "^3.922.0",
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.3.0",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "aws-lambda": "^1.0.7",
    "aws-serverless-fastify": "^3.1.3",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "graphql": "^16.12.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.2",
    "lambda-multipart-parser": "^1.0.1",
    "nodemailer": "^7.0.10",
    "postgres": "^3.4.7",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/aws-lambda": "^8.10.157",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "serverless-api-gateway-throttling": "^2.0.3",
    "serverless-prune-plugin": "^2.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      ".e2e-spec.ts$"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}